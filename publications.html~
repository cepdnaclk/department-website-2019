                $('.entry-content').hide();

                $('a.read').click(function () {
                    $(this).parent('.excerpt').hide();
                    $(this).closest('.tenant').find('.entry-content').slideDown('fast');
                    return false;
                });
                $('a.read-less').click(function () {
                    $(this).parent('.entry-contents').slideUp('fast');
                    $(this).parent('.entry-content').slideUp('fast');
                    $(this).closest('.tenant').find('.excerpt').show();
                    return false;
                });

